{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "font-awesome@6.2.0",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "library": "font-awesome-animation@1.1.0",
      "destination": "wwwroot/lib/font-awesome-animation/"
    },
    {
      "provider": "jsdelivr",
      "library": "canvas-confetti@1.6.0",
      "destination": "wwwroot/js/canvas-confetti/"
    },
    {
      "provider": "jsdelivr",
      "library": "js-cookie@3.0.1",
      "destination": "wwwroot/js/js-cookie/"
    }
  ]
}